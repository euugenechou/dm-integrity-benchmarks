[global]
direct=1
directory=${TEST_MNT}
end_fsync=1
filename=test.dat
group_reporting
ioengine=libaio
overwrite=1
ramp_time=5s
runtime=20s
size=1G
startdelay=5s
stonewall
time_based

[seq-1m-q8-t1-read]
bs=1M
fsync_on_close=1
iodepth=8
numjobs=1
rw=read

[seq-1m-q8-t1-write]
bs=1M
fsync_on_close=1
iodepth=8
numjobs=1
rw=write

[rnd-4k-q32-t16-read]
bs=4K
fsync=1
iodepth=32
numjobs=16
rw=randread

[rnd-4k-q32-t16-write]
bs=4K
fsync=1
iodepth=32
numjobs=16
rw=randwrite
